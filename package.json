{
  "name": "new-agent-demo",
  "version": "1.0.0",
  "description": "AI Agent Demo Platform with Office 365 Authentication",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd backend && npm run dev",
    "client:dev": "cd frontend && npm start",
    "server:install": "cd backend && npm install",
    "client:install": "cd frontend && npm install",
    "install:all": "npm install && npm run server:install && npm run client:install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["react", "nodejs", "postgresql", "office365", "ai-agents"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}