{
  "name": "new-agent-demo-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Agent Demo Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:unit": "jest --testPathPattern=tests/unit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-azure-ad": "^4.3.5",
    "passport-jwt": "^4.0.1",
    "node-cache": "^5.1.2",
    "uuid": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "express-session": "^1.17.3",
    "connect-session-sequelize": "^7.1.7",
    "axios": "^1.6.2",
    "joi": "^17.11.0",
    "isomorphic-dompurify": "^2.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "sqlite3": "^5.1.6",
    "@types/jest": "^29.5.8"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "testMatch": [
      "<rootDir>/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/tests/**",
      "!server.js",
      "!coverage/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  },
  "keywords": ["nodejs", "express", "postgresql", "office365"],
  "author": "",
  "license": "MIT"
}